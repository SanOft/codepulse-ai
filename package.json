  "scripts": {
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/server.ts",
    "dev:with-ngrok": "node scripts/start-dev-with-ngrok.js",
    "setup:webhook": "node scripts/setup-webhook.js",
    "install:ngrok": "powershell -ExecutionPolicy Bypass -File scripts/download-ngrok.ps1",
    "setup:ngrok": "node scripts/setup-ngrok-auth.js",
    "ngrok": "node scripts/start-ngrok.js",
    "webhook:info": "node scripts/get-webhook-url.js",
    "build": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
